<!--Project content-->
<div class="container justify-content-md-center">
  <h1><%= title %></h1>

 <div class="row card">
    <div class="card-header">
      Bug Smasher
    </div>  
    <!--carousel-->
    <div id="carouselExampleIndicators" class="carousel slide col-md-5 col-sm-8" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block w-100" src="/Assets/images/bug-smasher1.PNG" alt="First slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/Assets/images/bug-smasher2.PNG" alt="Second slide">
        </div>
      </div>
      <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
 </div>

 <div class="row card">
  <!--carousel-->
    <div id="carouselExampleIndicators2" class="carousel slide col-md-5 col-sm-8" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block w-100" src="/Assets/images/real-estate1.png" alt="First slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/Assets/images/real-estate2.png" alt="Second slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/Assets/images/real-estate3.png" alt="Third slide">
        </div>
      </div>
      <a class="carousel-control-prev" href="#carouselExampleIndicators2" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleIndicators2" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
</div>
